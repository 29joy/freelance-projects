python test_playwright_douyin.py --excel "data_Sept 3.xlsx"

python test_playwright_douyin.py --excel "data_Sept 3.xlsx" --sheet "high impact"

python test_playwright_douyin_v2.py --excel "data_Sept 3.xlsx" --sheet "high impact" --limit 3

# 按需修改 configs/default.yaml 中的 excel 路径/列名候选等
python -m cli.main --config configs/default.yaml --excel "data_Sept 3.xlsx" --sheet "high impact" --enable-backfill
python -m cli.main --config configs/default.yaml --excel "data_Sept_URL_NULL.xlsx" --sheet "high impact" --enable-backfill
python -m cli.main --config configs/default.yaml --excel "data_Sept_example.xlsx" --sheet "high impact" --enable-backfill


# 只跑第 0~100 行；开启回填
python -m cli.main --config configs/default.yaml --excel "data_Sept 3.xlsx" --sheet "high impact" --enable-backfill --start 55 --end 85


